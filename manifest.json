{
  "manifest_version": 3,
  "name": "AniList NekoSama Search",
  "version": "1.0",
  "description": "Adds a button to AniList anime pages to search on Neko-sama",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://anilist.co/anime/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "host_permissions": [
    "https://anilist.co/*"
  ]
}
