{
  "manifest_version": 3,
  "name": "AniSama",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://anilist.co/anime/*",
    "https://corsproxy.io/*",
    "https://neko-sama.fr/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
